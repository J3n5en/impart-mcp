    "tsup": "^8.5.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "ai": "^6.0.35",
    "ai-sdk-provider-claude-code": "^3.2.1",
    "ai-sdk-provider-codex-cli": "^1.0.4",
